# ElevenLabs MCP Server Configuration
# Copy this file to .env and fill in your values

# Required: ElevenLabs API Key
ELEVENLABS_API_KEY=sk_your_api_key_here

# Optional: Default output directory for generated audio files
# If not set, files will be saved to current working directory
OUTPUT_DIR=./output

# Optional: Logging level (debug, info, warn, error)
LOG_LEVEL=info

# Optional: Maximum retries for API calls (default: 3)
MAX_RETRIES=3

# Optional: Base delay in ms for exponential backoff (default: 1000)
RETRY_DELAY_MS=1000
